# Telegram Configuration
# 1. Open Telegram and search for "BotFather"
# 2. Send /newbot command
# 3. Follow instructions to get your token
# 4. Paste the token below

TELEGRAM_BOT_TOKEN=your_token_here_from_botfather

# Your Telegram Chat ID (for cross-channel messaging)
# Message @userinfobot on Telegram to get your chat ID
TELEGRAM_OWNER_CHAT_ID=

# Optional: LLM Keys
OPENROUTER_API_KEY=
OPENROUTER_MODEL=
NVIDIA_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=

# A2A (Agent-to-Agent) Auth Token (optional)
A2A_AUTH_TOKEN=

# Brave Search API (get free key at https://api.search.brave.com/)
BRAVE_SEARCH_API_KEY=

# Serper API (https://serper.dev)
SERPER_API_KEY=
